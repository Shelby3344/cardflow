# ==================================
# CARDFLOW - .gitignore
# ==================================

# ===== Node.js =====
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*
.pnpm-debug.log*

# ===== Environment Variables =====
.env
.env.local
.env.*.local
.env.development.local
.env.test.local
.env.production.local

# Manter .env.example e .env.production como templates
!.env.example
!.env.production

# ===== Next.js (Frontend) =====
frontend/.next/
frontend/out/
frontend/build/
frontend/.turbo/
frontend/dist/
.vercel
.next/
out/

# ===== Laravel (Backend) =====
backend/vendor/
backend/node_modules/
backend/.env
backend/.env.backup
backend/.env.production
backend/.phpunit.result.cache
backend/Homestead.json
backend/Homestead.yaml
backend/auth.json
backend/npm-debug.log
backend/yarn-error.log

# Laravel Storage
backend/storage/*.key
backend/storage/app/*
!backend/storage/app/.gitignore
!backend/storage/app/public/
backend/storage/framework/cache/*
!backend/storage/framework/cache/.gitignore
backend/storage/framework/sessions/*
!backend/storage/framework/sessions/.gitignore
backend/storage/framework/testing/*
!backend/storage/framework/testing/.gitignore
backend/storage/framework/views/*
!backend/storage/framework/views/.gitignore
backend/storage/logs/*
!backend/storage/logs/.gitignore

# Laravel Public
backend/public/hot
backend/public/storage
backend/public/build

# ===== Docker =====
docker-compose.override.yml
mysql_data/
postgres_data/
redis_data/
backend_storage/
backend_bootstrap_cache/
nginx_cache/

